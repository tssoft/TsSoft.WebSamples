<?xml version="1.0"?>
<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">

  <connectionStrings xdt:Transform="Replace" configProtectionProvider="ProductionProvider">
    <EncryptedData Type="http://www.w3.org/2001/04/xmlenc#Element"
      xmlns="http://www.w3.org/2001/04/xmlenc#">
      <EncryptionMethod Algorithm="http://www.w3.org/2001/04/xmlenc#tripledes-cbc" />
      <KeyInfo xmlns="http://www.w3.org/2000/09/xmldsig#">
        <EncryptedKey xmlns="http://www.w3.org/2001/04/xmlenc#">
          <EncryptionMethod Algorithm="http://www.w3.org/2001/04/xmlenc#rsa-1_5" />
          <KeyInfo xmlns="http://www.w3.org/2000/09/xmldsig#">
            <KeyName>Rsa Key</KeyName>
          </KeyInfo>
          <CipherData>
            <CipherValue>ZQTBlR3cHabeoFFsJuJO2fBPLr4V3bBdjD9M6o6OLSmuahVa3Jz7r+vockD1fq1wCoc6b/OMW2twX48D2ct+tKpM6k3F12+3o4HGOd47HbtH6zZdtBV3dYltkoNZUJ/r35O6LWxGtuTNe/5Du6LG5V7r7nKxNDeoUzEcIMLMcRc=</CipherValue>
          </CipherData>
        </EncryptedKey>
      </KeyInfo>
      <CipherData>
        <CipherValue></CipherValue>
      </CipherData>
    </EncryptedData>
  </connectionStrings>

  <socialSettings xdt:Transform="Replace" configProtectionProvider="ProductionProvider">
    <EncryptedData Type="http://www.w3.org/2001/04/xmlenc#Element"
      xmlns="http://www.w3.org/2001/04/xmlenc#">
      <EncryptionMethod Algorithm="http://www.w3.org/2001/04/xmlenc#tripledes-cbc" />
      <KeyInfo xmlns="http://www.w3.org/2000/09/xmldsig#">
        <EncryptedKey xmlns="http://www.w3.org/2001/04/xmlenc#">
          <EncryptionMethod Algorithm="http://www.w3.org/2001/04/xmlenc#rsa-1_5" />
          <KeyInfo xmlns="http://www.w3.org/2000/09/xmldsig#">
            <KeyName>Rsa Key</KeyName>
          </KeyInfo>
          <CipherData>
            <CipherValue>y7qVhI+zJsxKn3LbWFmtxdsG8QfmlpGELUsD3QA5cr0yvBFkdGilAmO9o+rk7HDjZKhXNnk9utqXchaHY1yGro4GJJvGFV/mO5qk9kEU9Dd6CP4h+OCnouS2Iw2jt3AZpUCx8miBm3j23a3wOkL5Jh27hBuoYCn371X86lM/JsU=</CipherValue>
          </CipherData>
        </EncryptedKey>
      </KeyInfo>
      <CipherData>
        <CipherValue></CipherValue>
      </CipherData>
    </EncryptedData>
  </socialSettings>

  <appSettings>
    <!-- Меняем атрибут value у AppBaseUrl -->
    <add key="AppBaseURL" value="http://my-domain.com"
         xdt:Transform="SetAttributes(value)"
         xdt:Locator="Match(key)" />
    <!--
      Вставляем элемент add key="ShareFolder" перед элементом add, находящимся в configuration->appSettings, и имеющим аттрибут key="MvcMailer.BaseURL"
    -->
    <add key="ShareFolder" value="~/shared"
         xdt:Transform="InsertBefore(/configuration/appSettings/add[@key='MvcMailer.BaseURL'])" />
  </appSettings>
  <!--Вставляем секцию System.net с настройками email-->
  <system.net xdt:Transform="Insert">
    <mailSettings>
      <smtp from="test@gmail.com">
        <network enableSsl="true" host="smtp.gmail.com" port="587" userName="test@gmail.com" password="506484654" />
      </smtp>
    </mailSettings>
  </system.net>
  <system.web>
    <!--Удаляем аттрибут debug у всех элементов compilation в узле system.web-->
    <compilation xdt:Transform="RemoveAttributes(debug)" />
  </system.web>
</configuration>